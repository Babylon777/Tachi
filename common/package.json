{
	"name": "tachi-common",
	"version": "0.15.2",
	"description": "Common Types & Utilities for Tachi",
	"main": "js/index.js",
	"files": [
		"js/**/*"
	],
	"scripts": {
		"test": "tap",
		"build": "tsc",
		"prepublish": "tsc",
		"lint": "eslint . --ext .ts --fix"
	},
	"author": "zkldi",
	"license": "MIT",
	"dependencies": {
		"@types/express-serve-static-core": "4.17.29",
		"@types/mongodb": "3.6.18",
		"prudence": "^0.9.8"
	},
	"devDependencies": {
		"@types/tap": "^15.0.7",
		"tap": "15.1.6"
	},
	"nyc": {
		"reporter": [
			"html",
			"lcov"
		],
		"all": true,
		"include": [
			"src/*/!(test-utils)/**/*.ts"
		],
		"exclude": [
			"src/**/*.test.ts",
			"src/external/mongo/schemas.ts"
		]
	}
}